        <!-- FEATURED PRODUCT SECTION START -->
        <div class="featured-product-section section-bg-tb pt-80 pb-55">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="section-title text-start mb-20">
                            <h2 class="uppercase">product list</h2>
                            <h6></h6>
                        </div>
                        <div class="featured-product">
                            <div class="active-featured-product slick-arrow-2">
                                <% for (let i = 0; i < Math.min(product.length, 4); i++) { %>
                                <div class="product-item product-item-2">
                                    <div class="product-img">
                                        <a href="/details?productId=<%= product[i]._id %>">
                                            <img src="/catimages/<%= product[i].pimg[0] %>" class="card-img-top" alt="<%= product[i].pname %>" style="height: 300px; object-fit: cover;">
                                        </a>
                        </a>
                                    </div>
                                    <div class="product-info">
                                       
                                        <div class="card-body text-center">
                                            <h5 style="font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif; color:black"><%= product[i].pname %></h5>
                                            <% if(!product[i].proOffer[0]?.productOfferAmt && !product[i].catOffer[0]?.categoryofferAmt) { %>
                                                <h5 style="color: black;">₹<%= product[i].price %></h5>
                                            <% } else if(proffer || catoffer){ %>
                                                <%   
                                                    const price = parseFloat(product[i].price); 
                                                    const productOffer = product[i].proOffer[0]?.productOfferAmt || 0; 
                                                    const categoryOffer = product[i].catOffer[0]?.categoryofferAmt || 0;
                                                    const highestOffer = Math.max(productOffer, categoryOffer);
                                                    const discountedPrice = price * (100 - highestOffer) / 100;
                                                %>
                                                <h6>
                                                    <span style="color: black;">₹<%= discountedPrice.toFixed(0) %></span> &nbsp;
                                                    <span class="original-price" >₹<%= product[i].price %></span>   
                                                    <span style="color: green;">(<%= highestOffer %>% off)</span>
                                                </h6>
                                            <% } %>
                                    </div>
                                    </div>
                                    <ul class="action-button">
                                        <li>
                                            <a href="#" title="Wishlist"><i class="zmdi zmdi-favorite"></i></a>
                                        </li>
                                        <li>
                                            <a href="#" data-bs-toggle="modal"  data-bs-target="#productModal" title="Quickview"><i class="zmdi zmdi-zoom-in"></i></a>
                                        </li>
                                        <li>
                                            <a href="#" title="Compare"><i class="zmdi zmdi-refresh"></i></a>
                                        </li>
                                        <li>
                                            <a href="#" title="Add to cart"><i class="zmdi zmdi-shopping-cart-plus"></i></a>
                                        </li>
                                    </ul>
                                </div>
                                <% } %>
                               
                            </div>
                        </div>          
                    </div>
                </div>
            </div>            
        </div>
